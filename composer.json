{
    "name": "kirschbaum-development/nova-mail",
    "description": "A Laravel Nova action for sending mail.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "dmitrybubyakin/nova-medialibrary-field": "dev-feat-mix",
        "saintsystems/nova-resource-group-menu": "dev-feat-localization",
        "spatie/laravel-medialibrary": "^7.14"
    },
    "autoload": {
        "psr-4": {
            "KirschbaumDevelopment\\NovaMail\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KirschbaumDevelopment\\NovaMail\\NovaMailServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Assiclick/nova-resource-group-menu"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Assiclick/nova-medialibrary-field"
        }
    ]
}
